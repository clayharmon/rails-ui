{
  "name": "rails-ui",
  "version": "1.0.0",
  "description": "A GUI for all rails CLI commands",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "webpack --mode production",
    "start": "yarn build && yarn server",
    "client": "webpack serve",
    "server": "tsc -p tsconfig.server.json && node dist/server/",
    "dev": "concurrently \"yarn server-dev\" \"yarn client\"",
    "server-dev": "nodemon --watch src"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/eslint": "^8.4.1",
    "@types/express": "^4.17.13",
    "@types/express-ws": "^3.0.1",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/shell-escape": "^0.2.0",
    "@types/shelljs": "^0.8.11",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "babel-loader": "^8.2.4",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.12.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "nodemon": "^2.0.15",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "shelljs": "^0.8.5",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.6.0",
    "core-js": "^3.21.1",
    "express": "^4.17.3",
    "express-ws": "^5.0.2",
    "node-pty": "^0.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "^0.13.9",
    "ruby-spawn": "^1.3.0",
    "shell-escape": "^0.2.0",
    "ws": "^8.5.0"
  }
